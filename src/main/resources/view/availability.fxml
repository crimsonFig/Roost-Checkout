<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox id="availabilityRoot" fx:id="rootNode" alignment="TOP_CENTER" prefHeight="650.0" prefWidth="450.0" styleClass="trayVBox" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <ImageView fx:id="ivLogo" fitHeight="60.0" fitWidth="450.0" pickOnBounds="true" preserveRatio="true">
    </ImageView>
    <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="30.0">
        <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Availability" textAlignment="CENTER">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
    </HBox>
    <TableView fx:id="tvStationsAvailability" prefHeight="280.0">
        <columns>
            <TableColumn fx:id="tcStationName" editable="false" prefWidth="200.0" text="Station" />
            <TableColumn fx:id="tcStationAmount" editable="false" minWidth="0.0" prefWidth="100.0" sortable="false" text="Amount #" />
            <TableColumn fx:id="tcStationAvailability" editable="false" prefWidth="150.0" sortable="false" text="Availability" />
        </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
    </TableView>
    <TableView fx:id="tvEquipAvailability" prefHeight="280.0">
        <columns>
            <TableColumn fx:id="tcEquipName" editable="false" prefWidth="200.0" text="Equipment" />
            <TableColumn fx:id="tcEquipAmount" editable="false" prefWidth="100.0" sortable="false" text="Amount #" />
            <TableColumn fx:id="tcEquipAvailability" editable="false" prefWidth="150.0" sortable="false" text="Availability" />
        </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
    </TableView>
   <padding>
      <Insets bottom="30.0" left="30.0" right="30.0" />
   </padding>
</VBox>
