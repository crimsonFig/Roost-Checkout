<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="base" prefHeight="543.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <bottom>
    <HBox alignment="CENTER_RIGHT" prefHeight="55.0" prefWidth="600.0">
      <children>
        <Button id="menuButton" fx:id="checkInButton" mnemonicParsing="false" onAction="#checkInAction" text="Check In">
          <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x1" />
          </HBox.margin>
        </Button>
        <Button id="menuButton" fx:id="checkOutButton" mnemonicParsing="false" onAction="#checkOutAction" text="Check Out" HBox.margin="$x1" />
        <Button id="menuButton" fx:id="availibilityButton" mnemonicParsing="false" onAction="#availibilityAction" text="Availibility" HBox.margin="$x1" />
        <Button id="waitListButton" fx:id="waitlistButton" mnemonicParsing="false" onAction="#waitlistAction" text="Waitlists">
          <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x1" />
          </HBox.margin>
        </Button>
        <Button id="menuButton" fx:id="bellButton" mnemonicParsing="false" onAction="#bellAction" text="Alerts">
          <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x1" />
          </HBox.margin>
        </Button>
      </children>
    </HBox>
  </bottom>
  <top>
    <VBox prefHeight="143.0" prefWidth="600.0" style="&#10;">
      <children>
        <MenuBar>
          <menus>
            <Menu mnemonicParsing="false" text="Roost">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#checkOutAction" text="Check Out" />
                <MenuItem mnemonicParsing="false" onAction="#checkInAction" text="Check In" />
                <MenuItem mnemonicParsing="false" onAction="#availibilityAction" text="Availibility" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Alerts">
              <items>
                <MenuItem mnemonicParsing="false" text="View Alerts" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Inventory">
              <items>
                <MenuItem mnemonicParsing="false" text="Add" />
              </items>
            </Menu>
          </menus>
        </MenuBar>
        <Label alignment="CENTER" contentDisplay="CENTER" text="Roost Checkout" textAlignment="CENTER">
          <font>
            <Font size="40.0" />
          </font>
        </Label>
      </children>
    </VBox>
  </top>
</BorderPane>
